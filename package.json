{
  "name": "freezly",
  "version": "0.0.6",
  "description": "Small utility for deeply freezing objects in JavaScript.",
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "build": "nodely -s ./src -o ./lib",
    "test": "lintly src && flow && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dogma-io/freezly.git"
  },
  "keywords": [
    "object",
    "freeze",
    "immutable",
    "immutability"
  ],
  "author": "Matthew Dahl <https://github.com/dogma-io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dogma-io/freezly/issues"
  },
  "homepage": "https://github.com/dogma-io/freezly#readme",
  "devDependencies": {
    "flow-bin": "0.59.0",
    "jest-cli": "21.2.1",
    "lintly": "0.0.4",
    "nodely": "0.0.11"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testRegex": "/__tests__/.*-test\\.js$"
  }
}
